<div class="container">
    <div class="row">
        <div class="col-md-12">
            <p id="notice"><%= notice %></p>

            <h1>Courses: <%= @courses.count %></h1>

            <div class="card-columns">
                <% @courses.each do |course| %>
                <%= render 'courses/course', course: course %>
                <% end %>
            </div>




        </div>
    </div>
</div>